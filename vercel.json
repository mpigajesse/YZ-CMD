{
  "version": 2,
  "builds": [
    {
      "src": "config/wsgi_vercel.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/static/(.*)",
      "dest": "/static/$1"
    },
    {
      "src": "/media/(.*)",
      "dest": "/media/$1"
    },
    {
      "src": "/(.*)",
      "dest": "config/wsgi_vercel.py"
    }
  ],
  "env": {
    "DJANGO_SETTINGS_MODULE": "config.settings"
  },
  "functions": {
    "config/wsgi_vercel.py": {
      "maxDuration": 30
    }
  }
} 